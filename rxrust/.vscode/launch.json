  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in library 'rxrust'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--lib",
          "--package=rxrust"
        ],
        "filter": {
          "name": "rxrust",
          "kind": "lib"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}",
      "sourceMap": {
        "/rustc/": "/Users/${env:USERNAME}/.rustup/toolchains/stable-x86_64-apple-darwin/lib/rustlib/src/rust/",
        "${cwd}/deps/": "/Users/${env:USERNAME}/.cargo/.registry/src/*/",
      }
    }
  ]
}